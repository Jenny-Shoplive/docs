{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Alive",
  "colors": {
    "primary": "#10111A",
    "light": "#ffffff"
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "favicon": {
    "light": "/favicon_light.svg",
    "dark": "/favicon_dark.svg"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "icons": {
    "library": "lucide"
  },
  "fonts": {
    "family": "Pretendard Variable",
    "weight": 450,
    "source": "https://github.com/Jenny-Shoplive/docs/blob/dfc4e51c477dbe59445e3e476b5d57897ed1e5d4/static/fonts/PretendardVariable.woff2",
    "format": "woff2",
    "heading": {
      "family": "DM Sans"
      }
  },
  "navigation": {
    "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Foundation",
            "pages": [
              "foundation/color",
              "foundation/typography"
            ]
          },
          {
            "group": "Components",
            "pages": [
              "components/avatar",
              "components/badge",
              "components/banner",
              "components/blankslate",
              "components/breadcrumbs"
            ]
          }
        ]
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Shoplive",
      "href": "https://shoplive.cloud"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
